<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.tec.myhomeworkout">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Peso"
            android:label="@string/altura"
            android:parentActivityName=".Altura"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Altura"
            android:label="@string/nacimiento"
            android:parentActivityName=".Nacimiento"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".PaginaInicial"
            android:screenOrientation="portrait"
            android:theme="@style/noToolBar">
        </activity>
        <activity
            android:name=".ProfileAct"
            android:label="@string/perfil"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Nacimiento"
            android:label="@string/genero"
            android:parentActivityName=".Genero"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Genero">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogInAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/splashTheme" />
    </application>

</manifest>